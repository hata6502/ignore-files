{
  "name": "ignore-files",
  "version": "1.0.0",
  "description": "A CLI to ignore files by .*ignore.",
  "keywords": [
    "ignore",
    "gitignore",
    "cli",
    "filter",
    "file"
  ],
  "homepage": "https://github.com/hata6502/ignore-files#readme",
  "bugs": {
    "url": "https://github.com/hata6502/ignore-files/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hata6502/ignore-files.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/hata6502"
  },
  "license": "MIT",
  "author": "Tomoyuki Hata",
  "bin": {
    "ignore": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fix": "prettier --write .",
    "test": "prettier --check . && tsc --noEmit"
  },
  "dependencies": {
    "get-stdin": "^8.0.0",
    "ignore": "^5.1.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.33",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.3",
    "typescript": "^4.2.3"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
